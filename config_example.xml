<Config>
    <!-- The address which HTTP will bind to -->
    <address>127.0.0.1:9011</address>

    <!-- Credentials -->
    <username>username</username>
    <password>password</password>

    <!-- Database information-->
    <databaseAddress>127.0.0.1</databaseAddress>
    <databaseName>CMOC</databaseName>
    <assetsPath>assets</assetsPath>

    <!-- For debug purposes only-->
    <!-- Turns off the authentification middleware-->
    <auth_mode>false</auth_mode>

    <!-- OIDC credentials/data -->
    <oidc>
        <clientID></clientID>
        <clientSecret></clientSecret>
        <redirectURL>http://localhost:9011/panel/authorize</redirectURL>
        <scopes>openid profile</scopes>
        <provider></provider>
    </oidc>
</Config>